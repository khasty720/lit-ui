{
    "env": {
      "browser": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 13
    },
    "rules": {
      "max-len": ["error", 120, 2, {
        "ignoreUrls": true,
        "ignoreComments": false,
        "ignoreRegExpLiterals": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true
      }],
      "no-param-reassign": 0,
      "class-methods-use-this": 0,
      "object-curly-newline": [
        "error", {
          "consistent": true
        }
      ]
    }
  }
  